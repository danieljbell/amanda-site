n    img {\n      max-width: 20px;\n      max-height: 20px;\n    }\n  }\n\n  nav ul {\n    display: flex;\n    list-style-type: none;\n    padding-bottom: 2rem;\n  }\n\n  nav li {\n    position: relative;\n    &:after {\n      content: '|';\n      padding: 0 1rem;\n    }\n    &:last-child:after {\n      content: '';\n    }\n  }\n\n  nav button {\n    display: none;\n  }\n\n}\n\nnav {\n  background-color: rgba(20,32,119, 1);\n  position: sticky;\n  top: 0;\n  z-index: 100;\n}\n\n.site-nav {\n  display: flex;\n  justify-content: center;\n  padding: 0;\n  list-style-type: none;\n\n  a {\n    display: block;\n    padding: 1rem 2rem;\n    color: #fff;\n    text-decoration: none;\n  }\n\n}\n\n.pad-y {\n  padding-top: 5rem;\n  padding-bottom: 5rem;\n}\n\n.grid {\n  display: table;\n  width: 100%;\n\n  > * {\n    display: table-cell;\n    vertical-align: middle;\n  }\n\n  &.grid--two > * {\n    width: 50%;\n    &:first-child {\n      padding-right: 2.5rem;\n    }\n    &:last-child {\n      padding-left: 2.5rem;\n    }\n  }\n\n}\n\nh2 {\n  font-size: 2.4rem;\n}\n\n.headshot img {\n  border-radius: 50%;\n  max-width: 300px;\n  margin: 0 auto;\n  display: block;\n}\n\n#resume {\n  background-color: #f0f0f0;\n\n  .site-width {\n    max-width: 700px;\n  }\n\n  .button {\n    float: right;\n    font-size: 0.85rem;\n    padding: 0.5rem 1.25rem;\n  }\n\n}\n\n.button,\ninput[type=\"submit\"] {\n  font-family: inherit;\n  font-size: inherit;\n  border: 0;\n  cursor: pointer;\n  background-color: rgba(77,92,149, 1);\n  color: #ffffff;\n  text-decoration: none;\n  padding: 0.75rem 1.5rem;\n  display: inline-block;\n  border-radius: 8px;\n  transition: transform 150ms ease-out;\n\n  &:hover,\n  &:focus {\n    transform: translate(-2px, -2px) scale(1.02);\n  }\n\n}\n\n#contact .site-width {\n  max-width: 650px;\n  text-align: center;\n}\n\n#contact p {\n  margin-bottom: 2rem;\n}\n\n.field {\n  margin-bottom: 1rem;\n  \n  label {\n    clip: rect(0 0 0 0); \n    clip-path: inset(50%);\n    height: 1px;\n    overflow: hidden;\n    position: absolute;\n    white-space: nowrap; \n    width: 1px;\n  }\n\n}\n\ninput[type=\"text\"],\ninput[type=\"email\"],\ntextarea {\n  border: 1px solid #ccc;\n  padding: 0.5rem;\n  font-size: inherit;\n  font-family: inherit;\n  border-radius: 5px;\n  box-shadow: 3px 3px 10px rgba(black, 0.08);\n  width: 100%;\n  max-width: 350px;\n  color: inherit;\n}\n\n.work-experience {\n  padding: 0;\n  list-style-type: none;\n  margin-top: 1.5rem;\n  position: relative;\n\n  &:before {\n    content: 'Experience';\n    writing-mode: vertical-lr;\n    transform: rotate(180deg);\n    position: absolute;\n    right: calc(100% + 2rem);\n    text-transform: uppercase;\n    font-weight: bold;\n    opacity: 0.1;\n    font-size: 4rem;\n  }\n\n  li {\n    margin-bottom: 3rem;\n  }\n\n}\n\n.work-bullets {\n  list-style-type: disc;\n  padding-left: 1rem;\n\n  li {\n    font-size: 0.9em;\n    line-height: 1.2;\n    margin-bottom: 5px;\n  }\n\n}\n\n.title {\n  font-size: 1.4rem;\n  font-weight: bold;\n}\n\n.work-meta {\n  font-style: italic;\n  margin-bottom: 1rem;\n}\n\n.bump-out {\n  position: relative;\n  left: -5rem;\n  width: calc(100% + 10rem);\n\n  hr {\n    background-color: transparent;\n    border: 0;\n    border-top: 1px solid #bbb;\n  }\n\n}\n\n"},"lineCount":null}},"error":null,"hash":"73a2d9aa4b2633983fcc669abb025491","cacheData":{"env":{}}}